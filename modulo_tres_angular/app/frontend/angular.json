{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "taskmanager-pro": {
      "projectType": "application",
      "root": "src",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "options": {
            "outputPath": "dist/taskmanager-pro",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [],
            "tsConfig": "tsconfig.app.json",
            "assets": [],
            "styles": [],
            "scripts": []
          }
        },
        "serve": {
          "options": {
            "browserTarget": "taskmanager-pro:build"
          }
        }
      }
    }
  },
  "defaultProject": "taskmanager-pro"
}
